															$spot = mysqli_query($conexao, "UPDATE imparpar SET impar = impar +1 WHERE concurso = '$meucurso'");
															// ADICIONAR DEZENA EM PALPITE
															$apst = mysqli_query($conexao, "UPDATE aposta SET sexta = '$teimoz62' WHERE data = '$date_m'");
														}

														else{

															// TERCEIRA ORDEM
															$busqua63 = mysqli_query($conexao, "SELECT * FROM quadrante5 WHERE min > 40 AND max < 61 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$dezena4' AND dezena != '$dezena5' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' AND dezena != '$teimoz61' AND dezena != '$teimoz62' ORDER BY quant DESC LIMIT 0,1 ");
															$cbsqua63 = mysqli_num_rows($busqua63);

															if ($cbsqua63 > 0) {

																while ($wbs63 = mysqli_fetch_array($busqua63)) {
																	$teimoz63 = $wbs63['dezena'];
																}

																// VERIFICA SE A DEZENA PERTENCE AO GRUPO IMPAR
																$raca63 = mysqli_query($conexao, "SELECT * FROM impar WHERE dezena = '$teimoz63'");
																$negr63 = mysqli_num_rows($raca63);

																if ($negr63 > 0) {
																	
																	// SOMA O VALOR DE +1 EM IMPAR
																	$spot = mysqli_query($conexao, "UPDATE imparpar SET impar = impar +1 WHERE concurso = '$meucurso'");
																	// ADICIONAR DEZENA EM PALPITE
																	$apst = mysqli_query($conexao, "UPDATE aposta SET sexta = '$teimoz63' WHERE data = '$date_m'");
																}

																else{

																	// QUARTA ORDEM
																	$busqua64 = mysqli_query($conexao, "SELECT * FROM quadrante5 WHERE min > 40 AND max < 61 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$dezena4' AND dezena != '$dezena5' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' AND dezena != '$teimoz61' AND dezena != '$teimoz62' AND dezena != '$teimoz63' ORDER BY quant DESC LIMIT 0,1 ");
																	$cbsqua64 = mysqli_num_rows($busqua64);


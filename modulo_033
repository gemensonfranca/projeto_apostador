						$quad3 = mysqli_query($conexao, "SELECT * FROM quadrante3 WHERE dezena = '$dezena3'");
						$cqua3 = mysqli_num_rows($quad3);

						if ($cqua3 > 0) {

							// VERIFICA SE QUADRANTE3 EXCEDEU LIMITE
							$verqq3 = mysqli_query($conexao, "SELECT * FROM quadricont WHERE concurso = '$meucurso'");

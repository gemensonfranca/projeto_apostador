																				$busqua35 = mysqli_query($conexao, "SELECT * FROM quadrante3 WHERE min > 20 AND max < 41 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$dezena1' AND dezena != '$dezena2' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' AND dezena != '$teimoz31' AND dezena != '$teimoz32' AND dezena != '$teimoz33' AND dezena != '$teimoz34' ORDER BY quant DESC LIMIT 0,1 ");
																				$cbsqua35 = mysqli_num_rows($busqua35);


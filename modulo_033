// CONEXÃO

include "../../key_conection.php";

// RESGATAR ÚLTIMO PALPIPE
$palpx = mysqli_query($conexao, "SELECT * FROM aposta ORDER BY concurso DESC LIMIT 0,1");
while ($look = mysqli_fetch_array($palpx)) {

	$roob1   = $look['primeira'];
	$roob2   = $look['segunda'];
	$roob3   = $look['terceira'];
	$roob4   = $look['quarta'];
	$roob5   = $look['quinta'];
	$roob6   = $look['sexta'];
}

// RESGATAR ÚLTIMO RESULTADO
$resgt = mysqli_query($conexao, "SELECT * FROM concurso ORDER BY mega DESC LIMIT 0,1");
while ($owt = mysqli_fetch_array($resgt)) {
	
	$dez01  = $owt['bola1'];
	$dez02  = $owt['bola2'];
	$dez03  = $owt['bola3'];
	$dez04  = $owt['bola4'];
	$dez05  = $owt['bola5'];
	$dez06  = $owt['bola6'];
}

// PROXIMO CONCURSO
$sql = mysqli_query($conexao, "SELECT * FROM concurso ORDER BY mega DESC LIMIT 0,1");
while ($mega = mysqli_fetch_array($sql)) {
	$curso   = $mega['mega'];
}

// DADOS DO PRÓXIMO PALPITE
date_default_timezone_set('America/Sao_Paulo');
$data     = date('d/m/Y - H:i');
$meucurso = $curso + 1;
$codyp    = "033";

// VERIFICAR SE EXISTE PALPITE COM MESMO CONCURSO (BLOQUEIO DE DUPLICAÇÃO)
$ver_block = mysqli_query($conexao, "SELECT * FROM aposta WHERE concurso = '$meucurso'");
$num_block = mysqli_num_rows($ver_block);

if ($num_block > 0) {
	header("Location: ../../index.php?error=duplication");
}

else{

	$bola1 = "00";
	$bola2 = "00";
	$bola3 = "00";
	$bola4 = "00";
	$bola5 = "00";
	$bola6 = "00";

	$into  = mysqli_query($conexao, "INSERT INTO aposta(data, concurso, primeira, segunda, terceira, quarta, quinta, sexta) VALUES('$data', '$meucurso', '$bola1', '$bola2', '$bola3', '$bola4', '$bola5', '$bola6')");

	// RESGATE DE DATA PARA VERIFICAÇÃO
	$momen = mysqli_query($conexao, "SELECT * FROM aposta WHERE primeira = '00' ORDER BY id DESC LIMIT 0,1");

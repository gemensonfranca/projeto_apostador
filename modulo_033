	$resgtip = mysqli_query($conexao, "SELECT * FROM contip ORDER BY quant DESC LIMIT 0,1");
	while ($contbox = mysqli_fetch_array($resgtip)) {
		$codsip     = $contbox['cod'];
		$impares    = $contbox['impares'];
		$pares      = $contbox['pares'];
	}


	// GRUPO G2 (21-40)

	// DEZENA D1


	// RANK 21 ~ 40
	$query1 = mysqli_query($conexao, "SELECT * FROM rank WHERE min > 20 AND max < 41 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' ORDER BY quant DESC LIMIT 0,1 ");
	while ($luck1 = mysqli_fetch_array($query1)) {
		$dezena1  = $luck1['dezena'];

		// VERIFICAR SE DEZENA PERTENCE AO QUADRANTE (Q3)
		$quad1 = mysqli_query($conexao, "SELECT * FROM quadrante3 WHERE dezena = '$dezena1'");
		$cqua1 = mysqli_num_rows($quad1);

		if ($cqua1 > 0) {
			
			// INCLUI +1 NO QUADRICONT QUADR3
			$quact = mysqli_query($conexao, "UPDATE quadricont SET quadr3 = quadr3 +1 WHERE concurso = '$meucurso'");

			// VERIFICA SE A DEZENA PERTENCE AO GRUPO IMPAR
			$raca1 = mysqli_query($conexao, "SELECT * FROM impar WHERE dezena = '$dezena1'");
			$negr1 = mysqli_num_rows($raca1);

			if ($negr1 > 0) {

				// SOMA O VALOR DE +1 EM IMPAR
				$spot = mysqli_query($conexao, "UPDATE imparpar SET impar = impar +1 WHERE concurso = '$meucurso'");
				// ADICIONAR DEZENA EM PALPITE
				$apst = mysqli_query($conexao, "UPDATE aposta SET primeira = '$dezena1' WHERE data = '$date_m'");
			}

			else{

				// SOMA O VALOR DE +1 EM PAR
				$spot = mysqli_query($conexao, "UPDATE imparpar SET par = par +1 WHERE concurso = '$meucurso'");
				// ADICIONAR DEZENA EM PALPITE
				$apst = mysqli_query($conexao, "UPDATE aposta SET primeira = '$dezena1' WHERE data = '$date_m'");
			}
		}

		else{

			// INCLUI +1 NO QUADRICONT QUADR4
			$quact = mysqli_query($conexao, "UPDATE quadricont SET quadr4 = quadr4 +1 WHERE concurso = '$meucurso'");

			// VERIFICA SE A DEZENA PERTENCE AO GRUPO IMPAR
			$raca1 = mysqli_query($conexao, "SELECT * FROM impar WHERE dezena = '$dezena1'");

												// CONDIÇÃO FERRENHA PARA PAR

												// PRIMEIRA ORDEM
												$busqua31 = mysqli_query($conexao, "SELECT * FROM quadrante3 WHERE min > 20 AND max < 41 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$dezena1' AND dezena != '$dezena2' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' ORDER BY quant DESC LIMIT 0,1 ");

	// VERIFICAR 

	// DEZENA D5

	// BUSCA DE RANKEADA 
	$query5 = mysqli_query($conexao, "SELECT * FROM rank WHERE min > 40 AND max < 61 AND dezena != '$dez01' AND dezena != '$dez02' AND dezena != '$dez03' AND dezena != '$dez04' AND dezena != '$dez05' AND dezena != '$dez06' AND dezena != '$dezena4' AND dezena != '$roob1' AND dezena != '$roob2' AND dezena != '$roob3' AND dezena != '$roob4' AND dezena != '$roob5' AND dezena != '$roob6' ORDER BY quant DESC LIMIT 0,1 ");
	while ($luck5 = mysqli_fetch_array($query5)) {
		$dezena5  = $luck5['dezena'];

		// VERIFICAR SE DEZENA PERTENCE AO QUADRANTE (Q5)
		$quad5 = mysqli_query($conexao, "SELECT * FROM quadrante5 WHERE dezena = '$dezena5'");
		$cqua5 = mysqli_num_rows($quad5);

		if ($cqua5 > 0) {

			// VERIFICA SE QUADRANTE5 EXCEDEU LIMITE
			$verqq3 = mysqli_query($conexao, "SELECT * FROM quadricont WHERE concurso = '$meucurso'");
			while ($samsung3  = mysqli_fetch_array($verqq3)) {
				$result_quad3 = $samsung3['quadr5'];

				if ($result_quad3 < 1) {

					// INCLUI +1 NO QUADRICONT QUADR5
					$quact = mysqli_query($conexao, "UPDATE quadricont SET quadr5 = quadr5 +1 WHERE concurso = '$meucurso'");
					
					// VERIFICA SE A DEZENA PERTENCE AO GRUPO IMPAR
					$raca5 = mysqli_query($conexao, "SELECT * FROM impar WHERE dezena = '$dezena5'");
					$negr5 = mysqli_num_rows($raca5);

					if ($negr5 > 0) {

						// VERIFICA SE IMPAR EXCEDEU
						$cancinha5 = mysqli_query($conexao, "SELECT * FROM imparpar WHERE concurso = '$meucurso'");
						while ($reega5 = mysqli_fetch_array($cancinha5)) {
							$impar_xx5 = $reega5['impar'];

							if ($impar_xx5 < $impares) {
								
								// SOMA O VALOR DE +1 EM IMPAR
								$spot = mysqli_query($conexao, "UPDATE imparpar SET impar = impar +1 WHERE concurso = '$meucurso'");
								// ADICIONAR DEZENA EM PALPITE
								$apst = mysqli_query($conexao, "UPDATE aposta SET quinta = '$dezena5' WHERE data = '$date_m'");
							}

							else{

								// CONDIÇÃO FERRENHA PARA PAR


			$acima2  = $numerl2 + 1;
			$abaixo2 = $numerl2 - 1;
			$margem2 = $quantc2 - 4;

			// CONSULTA E REGISTRO DE VALOR ACIMA DA DEZENA ANTERIOR
			$consci2 = mysqli_query($conexao, "SELECT * FROM rank WHERE min = '$acima2' AND quant > '$margem2'");
			$connum2 = mysqli_num_rows($consci2);

			if ($connum2 > 0) {

				while ($band2 = mysqli_fetch_array($consci2)) {
					$dezena3 = $band2['dezena'];

					// VERIFICA SE A DEZENA PERTENCE AO GRUPO IMPAR
					$racas2 = mysqli_query($conexao, "SELECT * FROM impar WHERE dezena = '$dezena3'");
